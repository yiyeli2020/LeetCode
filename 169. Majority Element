class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count=1;
        int major=nums[0];
        for(int i=1;i<nums.size();i++){
            if(count==0) {
                major=nums[i];
                count++;
            }
            else if(major==nums[i])
            {
                count++;
            }  
            else {
                count--;
            }
            // cout<<"i="<<i<<"  count="<<count<<"   major="<<major<<endl;
            
        }
        return major;
    }
};
